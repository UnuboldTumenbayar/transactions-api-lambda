{
  "name": "TransactionsApiRole",
  "description": "IAM role for the mock transactions API lambda.",
  "tutorial": "Ground to Cloud for Mongolian Software Development Engineers",
  "MockDynamoPolicy": {
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "dynamodb:PutItem",
                "dynamodb:GetItem",
                "dynamodb:Scan",
                "dynamodb:Query"
            ],
            "Resource": "*"
        }
    ]
  },
  "MockS3Policy": {
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject"
            ],
            "Resource": "*"
        }
    ]
  }
}